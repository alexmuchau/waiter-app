generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tb_comanda {
  Codigo Int     @id
  Ativo  String? @db.VarChar(10)
}

model tb_mesa {
  Codigo Int     @id
  Ativo  String? @db.VarChar(10)
}

model tb_mesa_comanda {
  Codigo     Int  @id
  Id_Mesa    Int?
  Id_Comanda Int?
}

model tb_pedido {
  Codigo        Int     @id
  ID_Activity   Int?
  Id_Mesa       Int?
  Id_Comanda    Int?
  Importado_ERP String? @db.VarChar(10)
}

model tb_pedido_item {
  Codigo        Int     @id
  Id_Pedido     Int?
  Id_Produto    Int?
  Quantidade    Float?
  Unitario      Float?
  Total         Float?
  Importado_ERP String? @db.VarChar(10)
}
